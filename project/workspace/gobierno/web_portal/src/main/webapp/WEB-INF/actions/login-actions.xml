<?xml version="1.0" encoding="UTF-8"?>
<actions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="../schema/actions.xsd">
    <form-beans>
    </form-beans>
    <action-mappings>

        <action type="LoginAction" path="/Login.do">
            <forward name="success_gobierno" path="/login/successLoginGobierno.jsp"/>
            <forward name="success_consumer" path="/login/successLoginConsumer.jsp"/>
            <forward name="warning" path="/login/failParamsLogin.jsp"/>
            <forward name="failure" path="/login/failLogin.jsp"/>
        </action>

        <action type="LogoutAction" path="/Logout.do">
            <forward name="success" path="/home/home.jsp" redirect="true"/>
        </action>

    </action-mappings>
</actions>
